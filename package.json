{
  "name": "@youngjuning/react-native-template-mobx",
  "version": "0.63.1",
  "description": "Clean and minimalist React Native template for a quick start with TypeScript and Mobx.",
  "scripts": {
    "commit": "git cz",
    "release": "standard-version --release-as patch"
  },
  "files": [
    "template",
    "template.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngjuning/react-native-template-mobx.git"
  },
  "keywords": [
    "react-native",
    "typescript",
    "jest",
    "template",
    "boilerplate",
    "mobx"
  ],
  "author": {
    "name": "youngjuning",
    "url": "https://youngjuning.js.org",
    "email": "youngjuning@aliyun.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/youngjuning/react-native-template-mobx/issues"
  },
  "homepage": "https://github.com/youngjuning/react-native-template-mobx#readme",
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "commitizen": "^4.2.1",
    "commitlint-config-cz": "^0.13.2",
    "cz-customizable": "^6.3.0",
    "husky": "^4.3.0",
    "standard-version": "^9.0.0"
  }
}
